{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$ref": "#/definitions/Welcome4",
    "definitions": {
        "Welcome4": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "config_version": {
                    "type": "integer"
                },
                "allowed_outdate": {
                    "type": "string"
                },
                "mirrors_dir": {
                    "type": "string"
                },
                "vault_mirror": {
                    "type": "string"
                },
                "versions": {
                    "$ref": "#/definitions/Versions"
                },
                "optional_module_versions": {
                    "type": "object",
                    "patternProperties": {
                        "^.*$": {
                            "$ref": "#/definitions/Versions"
                        }
                    }
                },
                "duplicated_versions": {
                    "type": "object",
                    "patternProperties": {
                        "^.*$": {
                            "$ref": "#/definitions/Version"
                        }
                    },
                    "propertyNames": {
                        "$ref": "#/definitions/Version"
                    },
                    "additionalProperties": false
                },
                "vault_versions": {
                    "$ref": "#/definitions/Versions"
                },
                "required_protocols": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "arches": {
                    "type": "object",
                    "items": {
                        "$ref": "#/definitions/ArchesVersions"
                    }
                },
                "repos": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Repo"
                    }
                }
            },
            "required": [
                "allowed_outdate",
                "arches",
                "duplicated_versions",
                "required_protocols",
                "mirrors_dir",
                "repos",
                "vault_mirror",
                "vault_versions",
                "versions"
            ],
            "title": "Welcome4"
        },
        "Version": {
            "oneOf": [
                {
                    "pattern": "^[0-9]+(.[0-9]+(-beta)?|(-kitten)?)?$"
                }
            ]
        },
        "Repo": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "path": {
                    "type": "string"
                },
                "arches": {
                    "$ref": "#/definitions/Arches"
                },
                "versions": {
                    "$ref": "#/definitions/Versions"
                },
                "vault": {
                    "type": "boolean"
                }
            },
            "required": [
                "name",
                "path"
            ],
            "title": "Repo"
        },
        "ArchesVersions": {
            "type": "object",
            "additionalProperties": false,
            "patternProperties": {
                "^.*$": {
                    "$ref": "#/definitions/Arches"
                }
            },
            "title": "ArchesVersions"
        },
        "Arches": {
            "type": "array",
            "contains": {
                "type": "string",
                "enum": [
                    "x86_64",
                    "x86_64_v2",
                    "aarch64",
                    "ppc64le",
                    "s390x"
                ]
            }
        },
        "Versions": {
            "type": "array",
            "items": {
                "type": "string",
                "$ref": "#/definitions/Version"
            }
        }
    }
}
